{
  "name": "opendss-node-interface",
  "version": "0.0.7",
  "description": "An interface to communicate with OpenDSS (Using ActiveX)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --runInBand",
    "start": "ts-node ./src/index.ts",
    "typecheck": "tsc ./src/index.ts --noEmit",
    "typedoc": "typedoc --out docs src/index.ts",
    "build": "tsup",
    "format": "prettier ./src/**/*.ts --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "script": "ts-node ./script"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "winax": "^3.4.1"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/recommended": "^1.0.3",
    "@types/activex-opendssengine": "^4.0.0",
    "@types/eslint": "^8.44.8",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-define-config": "^2.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3"
  }
}
